{
  "expo": {
    "name": "DESIGNSNACK Laws & Patterns",
    "slug": "designsnack-laws-patterns",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons/ios/icon-1024.png",
    "scheme": "designsnacklawspatterns",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-screens/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/splash-screens/splash-ios.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "com.designsnack.lawspatterns",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/app-icons/android/adaptive-icon-foreground.png"
      },
      "splash": {
        "image": "./assets/splash-screens/splash-android.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "newArchEnabled": true,
      "package": "com.designsnack.lawspatterns"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/app-icons/ios/icon-1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-screens/splash.png",
          "imageWidth": 1242,
          "resizeMode": "native",
          "backgroundColor": "#1E88BC"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7110fed7-12c1-40f7-a56b-f43b283f8db2"
      }
    },
    "owner": "designsnack"
  }
}
